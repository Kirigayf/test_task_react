{
  "name": "test-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "bot": "nodemon --watch src/lib --exec ts-node --project tsconfig.bot.json src/lib/telegram-bot.ts",
    "dev-with-bot": "concurrently \"npm run dev\" \"npm run bot\"",
    "prod-with-bot": "concurrently \"npm run start\" \"npm run start-bot\""
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "dotenv": "^17.0.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.0",
    "next": "15.3.4",
    "node-cron": "^4.2.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sqlite3": "^3.1.11",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
